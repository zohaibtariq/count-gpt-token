{
    "name": "zohaibtariq/count-gpt-token",
    "description": "This is my package to count token of a gpt prompt",
    "keywords": [
        "zohaibtariq",
        "laravel",
        "count-gpt-token"
    ],
    "homepage": "https://github.com/zohaibtariq/count-gpt-token",
    "license": "MIT",
    "authors": [
        {
            "name": "Zohaib Tariq",
            "email": "zebwebdev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "ZohaibTariq\\CountGptToken\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZohaibTariq\\CountGptToken\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZohaibTariq\\CountGptToken\\CountGptTokenServiceProvider"
            ],
            "aliases": {
                "CountGptToken": "ZohaibTariq\\CountGptToken\\Facades\\CountGptToken"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
